+  /* http://h14s.p5r.org/2012/09/0x5f3759df.html?mwh=1 */
+  SIMDE__VECTORIZE
+  for (size_t i = 0 ; i < (sizeof(r.f32) / sizeof(r.f32[0])) ; i++) {
+    r.i32[i]  = INT32_C(0x5f3759df) - (a.i32[i] >> 1);
